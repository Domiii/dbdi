{
  "name": "dbdi",
  "version": "0.2.0",
  "description": "# Intro",
  "homepage": "https://github.com/Domiii/dbdi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Domiii/dbdi.git"
  },
  "author": "Domi",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "lint": "eslint -c .eslintrc.yml *.js src",
    "prepublish": "npm run build",
    "build-test": "cross-env NODE_ENV=test webpack --display-chunks --progress",
    "test": "npm run build-test && jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test-debug-win": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "npm run test -- --inspect-brk --runInBand"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "pluralize": "^7.0.0",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=16.5.2",
    "react-dom": ">=16.5.2",
    "lodash": "^4.17.11",
    "firebase": "^5.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "firebase": "^5.8.4",
    "glob": "^7.1.3",
    "jest": "^24.1.0",
    "jest-extended": "^0.11.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.0",
    "prop-types": "^15.7.2",
    "react": ">=16.5.2",
    "react-dom": ">=16.5.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3"
  },
  "bugs": {
    "url": "https://github.com/Domiii/dbdi/issues"
  },
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "react",
    "firebase",
    "databinding"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "<rootDir>"
    ],
    "verbose": true,
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
